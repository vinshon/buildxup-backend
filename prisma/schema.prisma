model User {
  id             Int     @id @default(autoincrement())
  name           String
  email          String? @unique
  phone          String  @unique
  profile        String?
  token          String?
  refresh_token  String?
  password       String
  verify_otp     String?
  is_active      Boolean @default(true)
  is_block       Boolean @default(false)
  status         String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}